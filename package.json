{
  "name": "gestao-musicas-e-bandas-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --base-href=/gestao-musicas-e-bandas-app/",
    "ghpages": "angular-cli-ghpages --dir=dist/gestao-musicas-e-bandas-app",
    "deploy": "npm run build & npm run ghpages",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "json:server": "json-server --watch db.json",
    "json:server:routes": "json-server --watch db.json --routes routes.json"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.1",
    "@angular/compiler": "^15.0.1",
    "@angular/core": "^15.0.1",
    "@angular/forms": "^15.0.1",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.1",
    "@angular/platform-browser-dynamic": "^15.0.1",
    "@angular/router": "^15.0.1",
    "@types/materialize-css": "^1.0.11",
    "ag-grid-angular": "^28.2.1",
    "ag-grid-community": "^28.2.1",
    "angular-cli-ghpages": "^1.0.5",
    "json-server": "^0.17.1",
    "local-storage": "^2.0.0",
    "material-icons": "^1.12.2",
    "materialize-css": "^1.0.0",
    "ngx-mask": "^14.2.4",
    "rxjs": "~7.5.7",
    "tslib": "^2.4.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.1",
    "@angular/cli": "~15.0.1",
    "@angular/compiler-cli": "^15.0.1",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0"
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  }
}
